<Window x:Class="Monitor.TaskControl.View.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"        
        xmlns:controls="clr-namespace:Monitor.TaskControl.UserControl"
        xmlns:resource="clr-namespace:Monitor.TaskControl.Resource"
        xmlns:sys="clr-namespace:System;assembly=mscorlib"
        mc:Ignorable="d" Height="720" Width="1280" 
        WindowStartupLocation="CenterScreen" ResizeMode="CanResize"
        BorderThickness="1" BorderBrush="Black" Loaded="Window_Loaded" MinWidth="1280" MinHeight="720" SizeChanged="Window_SizeChanged">

    <Window.Title>
        Monitor.Control
    </Window.Title>

    <Window.Resources>
        <Storyboard x:Key="SlideRight">
            <ThicknessAnimation Storyboard.TargetProperty="Margin" From="0,0,0,0" To="-230,0,0,0" AccelerationRatio=".9" Duration="0:0:0.3" />
        </Storyboard>
        <Storyboard x:Key="SlideLeft">
            <ThicknessAnimation Storyboard.TargetProperty="Margin" From="-230,0,0,0" To="0,0,0,0" AccelerationRatio=".9" Duration="0:0:0.3" />
        </Storyboard>
    </Window.Resources>
    <!--<Window.Background>
        <ImageBrush ImageSource="/Monitor.TaskControl;component/Resource/BG.png"/>
    </Window.Background>-->

    <Grid Margin="0,0,0,0" Background="#FF232323">
        <!--<Grid.Background>
            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                <GradientStop Color="Black" Offset="0"/>
                <GradientStop Color="#FF3E3E3E" Offset="1"/>
            </LinearGradientBrush>
        </Grid.Background>-->
        <Grid.ColumnDefinitions>

            <ColumnDefinition/>
            <ColumnDefinition Width="41"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="50" />
            <RowDefinition />
            <RowDefinition Height="4"/>
        </Grid.RowDefinitions>
        <!--<Grid.Resources>
            <resource:WindowStateToMinMaxButtonContentConverter x:Key="WinStateConv" />
        </Grid.Resources>-->
        <Grid Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="2" Margin="0, 0,-2,0" MouseMove="Grid_MouseMove"  Background="#FF40A6D1" >
            <!--<Grid.Background>
                <ImageBrush ImageSource="/Monitor.TaskControl;component/Resource/TitleBar.png"/>
            </Grid.Background>-->
            <controls:HeaderDecoration />
            <Image x:Name="AVARTA" Height="25" Width="25" Panel.ZIndex="1" HorizontalAlignment="Left" Margin="14,0,0,10" Source="/Resource/net-on.png" Stretch="Uniform" RenderTransformOrigin="0.396,1.005" VerticalAlignment="Bottom"/>

            <Label x:Name="Title" Content="TaskView Control 1.3.2"
                   HorizontalAlignment="Left" VerticalAlignment="Center" FontSize="25" FontWeight="SemiBold"
                   Foreground="White" Margin="43,4,0,4">
                <Label.Effect>
                    <DropShadowEffect Opacity="0.2" />
                </Label.Effect>
            </Label>
            <StackPanel VerticalAlignment="Center" Orientation="Horizontal"
                        Margin="350,1,517,9" Height="40">
                <DatePicker x:Name="date_Picker" Margin="0,12,0,0" Width="280" FontSize="17" Height="28"  HorizontalAlignment="Right" SelectedDateFormat="Long" Grid.Column="2" SelectedDateChanged="OnSelectedDateChaged" CalendarOpened="DatePicker_CalendarOpened" CalendarClosed="DatePicker_CalendarClosed" Focusable="False" />
                <Image x:Name="imgBack" Source="/Resource/Back.png" MouseLeftButtonDown="imgBackButton_Click" MouseMove="imgBackMouseMove" MouseLeave="imgBackMouseLeave" Width="25" Height="20" Margin="0,16,0,4" RenderTransformOrigin="1.302,1.035"/>
                <Image x:Name="imgForward" Source="/Resource/Forward.png" MouseLeftButtonDown="imgForwardButton_Click" MouseMove="imgForwardMouseMove" MouseLeave="imgForwardMouseLeave" Width="25" Height="20" Margin="4,16,0,4" RenderTransformOrigin="12.351,-0.392" Grid.ColumnSpan="2" Grid.Column="2"></Image>

                <Button x:Name="btnToday" Style="{DynamicResource RoundedSettingsButtonStyle1}" 
                        Foreground="White" Padding="0 0" Background="#FFE8850E"
                        BorderBrush="{x:Null}" BorderThickness="0" Width="60" Height="20" Content="Today" Margin="8,16,505,4" Click="imgTodayButton_Click"  RenderTransformOrigin="0.341,0.217" Grid.Column="3"  />
            </StackPanel>
            <Label x:Name="DiskSpace" Content="" Foreground="Yellow"
                   HorizontalAlignment="Left" VerticalAlignment="Center" FontSize="16" FontWeight="SemiBold"
                    Margin="780,5,0,0">
                <Label.Effect>
                    <DropShadowEffect Opacity="0.2" />
                </Label.Effect>
            </Label>
            <StackPanel Grid.ColumnSpan="2" Grid.Column="2" VerticalAlignment="Bottom" Orientation="Horizontal"
                        Margin="0,0,5,0" HorizontalAlignment="Right">
                <Button Content="" Style="{DynamicResource CloseMinMaxButtonStyle}" Width="40" Height="40" ToolTip="Hide"
                        BorderBrush="Transparent" Background="Transparent" Foreground="White"
                        FontFamily="pack://application:,,,/Resource/#FontAwesome" FontSize="20"
                        VerticalContentAlignment="Bottom" Click="Cancel_Click" Focusable="False"  />
                <Button x:Name="maxButton" Width="40" Height="40" ToolTip="Resize" Click="MaxmizeButton_OnClick" Focusable="False" Content="&#xF065;" Style="{DynamicResource CloseMinMaxButtonStyle}"
                        BorderBrush="Transparent" Background="Transparent" Foreground="White"
                        FontFamily="pack://application:,,,/Resource/#FontAwesome" FontSize="20"
                        VerticalContentAlignment="Bottom"  />
                <Button Width="40" Height="40" Click="CloseButton_OnClick" Focusable="False" Content="" Style="{DynamicResource CloseMinMaxButtonStyle}"
                        BorderBrush="Transparent" Background="Transparent" Foreground="White" ToolTip="Restart"
                        FontFamily="pack://application:,,,/Resource/#FontAwesome" FontSize="20"
                        VerticalContentAlignment="Bottom" />
            </StackPanel>



        </Grid>

        <TabControl Grid.Row="1" TabStripPlacement="Left" Margin="0,10,22,0" Grid.ColumnSpan="2" Grid.Column="0"
                        Style="{DynamicResource LeftTabColumnTabControlStyle}" Background="LightGray"
                        BorderBrush="{x:Null}">
            <TabItem Style="{DynamicResource LeftTabColumnTabItemStyle}"  BorderBrush="{x:Null}">
                <TabItem.Header>
                    <!--<StackPanel Orientation="Horizontal" HorizontalAlignment="Left">-->
                    <Label ToolTip="State" Content="&#xf007;" HorizontalContentAlignment="Center"
                               FontFamily="pack://application:,,,/Resource/#FontAwesome" Foreground="White"
                               VerticalAlignment="Center" FontSize="35"  Margin="8,4,8,4" />
                    <!--<Label ToolTip="State"
                               Content="State"
                               HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Foreground="#FF555555" VerticalAlignment="Center"
                               FontSize="18" Width="86" />-->
                    <!--</StackPanel>-->
                </TabItem.Header>
                <Grid >
                    <Grid.Background>
                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                            <GradientStop Color="#FFC4C4C4" />
                            <GradientStop Color="#FFEEEEEE" Offset="1" />
                            <GradientStop Color="#FFE4E4E4" Offset="0.11" />
                        </LinearGradientBrush>
                    </Grid.Background>
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="101*"/>
                            <RowDefinition Height="470*"/>
                        </Grid.RowDefinitions>
                        <StackPanel Grid.RowSpan="2">
                            <DataGrid x:Name="dataGrid" AutoGenerateColumns="False" CanUserResizeColumns="False" CanUserReorderColumns="False"  BorderBrush="#FF676868" Background="{DynamicResource Button.MouseOver.Background}">
                                <DataGrid.Resources>
                                    <Style BasedOn="{StaticResource {x:Type DataGridColumnHeader}}" TargetType="{x:Type DataGridColumnHeader}">
                                        <Setter Property="Background" Value="#AAE8850E" />
                                        <Setter Property="Foreground" Value="White"/>
                                        <Setter Property="FontWeight" Value="Bold"/>
                                        <Setter Property="Height" Value="27"/>
                                        <Setter Property="HorizontalContentAlignment" Value="Center" />
                                    </Style>
                                </DataGrid.Resources>
                                <DataGrid.Columns >
                                    <DataGridTextColumn Header="" Width="0.4*"/>
                                    <DataGridTextColumn Header="Name" Width="1*"/>
                                    <DataGridTextColumn Header="IP Address" Width="1.1*"/>
                                    <DataGridTextColumn Header="Computer Name" Width="1.4*"/>
                                    <DataGridTextColumn Header="OS Created" Width="1.6*"/>
                                    <DataGridTextColumn Header="Audio" Width="0.6*"/>
                                    <DataGridTextColumn Header="Visited URL" Width="0.7*"/>
                                    <DataGridTextColumn Header="Download" Width="0.7*"/>
                                    <DataGridTextColumn Header="Danger URL" Width="0.7*"/>
                                    <DataGridTextColumn Header="Forbidden" Width="0.7*"/>
                                    <DataGridTextColumn Header="Inspect" Width="1.8*"/>
                                    <DataGridTextColumn Header="Work Time" Width="0.7*"/>
                                    <DataGridTextColumn Header="Message" Width="0.7*"/>

                                </DataGrid.Columns>
                            </DataGrid>
                        </StackPanel>
                        <ScrollViewer VerticalScrollBarVisibility="Auto" Margin="0,30,0,0" Grid.RowSpan="2" Background="White">
                            <StackPanel x:Name="pnlPanels">
                            </StackPanel>
                        </ScrollViewer>
                    </Grid>
                </Grid>

            </TabItem>
