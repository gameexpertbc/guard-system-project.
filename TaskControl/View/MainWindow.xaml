                                                    <Style TargetType="{x:Type TextBlock}">
                                                        <Setter Property="VerticalAlignment" Value="Center" />
                                                        <Setter Property="Height" Value="25" />
                                                        <Setter Property="HorizontalAlignment" Value="Center" />
                                                    </Style>
                                                </DataGridTextColumn.ElementStyle>
                                            </DataGridTextColumn>
                                            <DataGridTextColumn Header="Allow URL" Binding="{Binding allowURL}" Width="*">
                                                <DataGridTextColumn.ElementStyle>
                                                    <Style TargetType="{x:Type TextBlock}">
                                                        <Setter Property="VerticalAlignment" Value="Center" />
                                                        <Setter Property="Height" Value="25" />
                                                        <Setter Property="HorizontalAlignment" Value="Center" />
                                                        <Setter Property="ToolTip" Value="{Binding allowURL}" />
                                                    </Style>
                                                </DataGridTextColumn.ElementStyle>
                                            </DataGridTextColumn>

                                        </DataGrid.Columns>
                                    </DataGrid>
                                </Grid>
                                <StackPanel Margin="20,10,10,250" Grid.Column="1">
                                    <Label Content="Allow URL : " Margin="-6,0,0,0" Foreground="#FF636363" FontSize="16" BorderThickness="1" VerticalAlignment="Center" FontWeight="SemiBold" />
                                    <TextBox HorizontalAlignment="Left" VerticalContentAlignment="Center"  FontSize="16" x:Name="txtAllowURL" VerticalAlignment="Center" Width="270" Height="28" Margin="0,0,0,15" />
                                    <Button Grid.Row="4" x:Name="btnAddURL" FontSize="16" Style="{DynamicResource RoundedSettingsButtonStyle}" Background="#FF40A9D1" HorizontalAlignment="Left"
                                    Foreground="White" BorderBrush="{x:Null}" BorderThickness="0" VerticalAlignment="Center"
                                     Content="Add Item" Click="btnAddURL_Click" Width="115" Height="33" Margin="80,15,0,0" />
                                    <Button Grid.Row="5" x:Name="btnDeletetURL" FontSize="16" Style="{DynamicResource RoundedSettingsButtonStyle}" Background="#FF40A9D1" HorizontalAlignment="Left"
                                    Foreground="White" BorderBrush="{x:Null}" BorderThickness="0" VerticalAlignment="Center"
                                     Content="Delete Item" Click="btnDeleteURL_Click"  Width="115" Height="32" Margin="80,15,0,0" />


                                </StackPanel>

                            </Grid>
                        </TabItem>
                        <TabItem Height="40" Margin="-1,9,0,0" Background="#e97600">
                            <TabItem.Header>
                                <StackPanel Orientation="Horizontal" Width="120">
                                    <Label Content="Forbidden URL" Margin="0,-10,0,0"
                               HorizontalContentAlignment="Center" MouseLeftButtonDown="OnShowForbiddenURL" VerticalContentAlignment="Center" 
                               FontSize="16" Width="120" Foreground="White"/>
                                </StackPanel>
                            </TabItem.Header>
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="2.8*"/>
                                    <ColumnDefinition Width="305"/>
                                </Grid.ColumnDefinitions>
                                <Grid Margin="10,0,0,5" Grid.Column="0">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="254*"/>
                                        <ColumnDefinition Width="581*"/>
                                    </Grid.ColumnDefinitions>
                                    <DataGrid x:Name="ForbiddenURL" IsReadOnly="True" SelectedItem="Flase" CanUserResizeRows="False" AutoGenerateColumns="False" AlternatingRowBackground="#FFe7f4fc" GridLinesVisibility="None" CanUserResizeColumns="False" CanUserReorderColumns="False" BorderBrush="#FF676868" Margin="-1,7,0,10" FontSize="14" Grid.ColumnSpan="2">
                                        <DataGrid.CellStyle>
                                            <Style TargetType="DataGridCell">
                                                <Setter Property="BorderThickness" Value="1,0,0,1"/>
                                                <Setter Property="BorderBrush" Value="LightBlue"/>
                                            </Style>
                                        </DataGrid.CellStyle>
                                        <DataGrid.Resources>
                                            <LinearGradientBrush x:Key="HeaderBorderBrush" StartPoint="0.5,0" EndPoint="0.5,1">
                                                <GradientStop Color="#EE01bbf2" Offset="0.32" />
                                            </LinearGradientBrush>
                                            <Style BasedOn="{StaticResource {x:Type DataGridColumnHeader}}" TargetType="{x:Type DataGridColumnHeader}">
                                                <Setter Property="Background" Value="{StaticResource HeaderBorderBrush}" />
                                                <Setter Property="Foreground" Value="White"/>
                                                <Setter Property="FontWeight" Value="Bold"/>
                                                <Setter Property="Height" Value="32"/>
                                                <Setter Property="HorizontalContentAlignment" Value="Center" />

                                            </Style>
                                        </DataGrid.Resources>
                                        <DataGrid.Columns >
                                            <DataGridTextColumn Header="No" Width="60" Binding="{Binding ID}">
                                                <DataGridTextColumn.ElementStyle>
                                                    <Style TargetType="{x:Type TextBlock}">
                                                        <Setter Property="VerticalAlignment" Value="Center" />
                                                        <Setter Property="Height" Value="25" />
                                                        <Setter Property="HorizontalAlignment" Value="Center" />
                                                    </Style>
                                                </DataGridTextColumn.ElementStyle>
                                            </DataGridTextColumn>
                                            <DataGridTextColumn Header="Forbidden URL's Index" Binding="{Binding forbiddenURL}" Width="*">
                                                <DataGridTextColumn.ElementStyle>
                                                    <Style TargetType="{x:Type TextBlock}">
                                                        <Setter Property="VerticalAlignment" Value="Center" />
                                                        <Setter Property="Height" Value="25" />
                                                        <Setter Property="HorizontalAlignment" Value="Center" />
                                                        <Setter Property="ToolTip" Value="{Binding forbiddenURL}" />
                                                    </Style>
                                                </DataGridTextColumn.ElementStyle>
                                            </DataGridTextColumn>

                                        </DataGrid.Columns>
                                    </DataGrid>
                                </Grid>
                                <StackPanel Margin="20,10,10,250" Grid.Column="1">
                                    <Label Content="Forbidden URL's Index : " Margin="-6,0,0,0" Foreground="#FF636363" FontSize="16" BorderThickness="1" VerticalAlignment="Center" FontWeight="SemiBold" />
                                    <TextBox HorizontalAlignment="Left" VerticalContentAlignment="Center"  FontSize="16" x:Name="txtForbiddenURL" VerticalAlignment="Center" Width="270" Height="28" Margin="0,0,0,15" />
                                    <Button Grid.Row="4" x:Name="btnAddFURL" FontSize="16" Style="{DynamicResource RoundedSettingsButtonStyle}" Background="#FF40A9D1" HorizontalAlignment="Left"
                                    Foreground="White" BorderBrush="{x:Null}" BorderThickness="0" VerticalAlignment="Center"
                                     Content="Add Item" Click="btnAddURL_Click" Width="115" Height="33" Margin="80,15,0,0" />
                                    <Button Grid.Row="5" x:Name="btnDeletetFURL" FontSize="16" Style="{DynamicResource RoundedSettingsButtonStyle}" Background="#FF40A9D1" HorizontalAlignment="Left"
                                    Foreground="White" BorderBrush="{x:Null}" BorderThickness="0" VerticalAlignment="Center"
                                     Content="Delete Item" Click="btnDeleteFURL_Click"  Width="115" Height="32" Margin="80,15,0,0" />


                                </StackPanel>

                            </Grid>
                        </TabItem>
                    </TabControl>
                </Grid>
            </TabItem>
        </TabControl>
        <Grid x:Name="grdMessageList" HorizontalAlignment="Left" Margin="0,0,-231,0" Grid.Row="1" Width="272" Grid.Column="1" RenderTransformOrigin="0.5,0.5">
            <StackPanel Orientation="Vertical" Margin="0,0,233,0">
                <Button x:Name="btnMessage" Margin="0,30,-7,0" 
                    Style="{DynamicResource RoundedMessageButtonStyle}" Background="#EE228B22"
                                        Foreground="White" BorderBrush="{x:Null}" BorderThickness="0" Click="btnMessage_Click" Width="25" Height="120" HorizontalAlignment="Right">
                    <StackPanel Orientation="Vertical">
                        <Ellipse x:Name="eliMessage" Fill="LightGray" HorizontalAlignment="Left" VerticalAlignment="Center" Width="10" Height="10" Margin="3,0,0,0" />
                        <TextBlock Text="Message  "  Margin="-5, 0,0,0" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="15">
                            <TextBlock.LayoutTransform>
                                <RotateTransform Angle="-90"/>
                            </TextBlock.LayoutTransform>
                        </TextBlock>
                    </StackPanel>
                </Button>
                <Button x:Name="btnHistory" Margin="21,3,-7,0" 
                    Style="{DynamicResource RoundedMessageButtonStyle}" Background="#EE228B22"
                                        Foreground="White" BorderBrush="{x:Null}" BorderThickness="0"
                                        Width="25" Click="btnHistory_Click" Height="120" HorizontalAlignment="Left">
                    <StackPanel Orientation="Vertical">
                        <TextBlock Margin="-5,0,0,0" Text="History" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="15">
                            <TextBlock.LayoutTransform>
                                <RotateTransform Angle="-90"/>
                            </TextBlock.LayoutTransform>
                        </TextBlock>
                    </StackPanel>
                </Button>
            </StackPanel>
            <StackPanel  Orientation="Vertical" Margin="44,0,0,0" HorizontalAlignment="Center" Background="#FF2B333B" Width="228">
                <ScrollViewer VerticalScrollBarVisibility="Auto"  Height="{Binding ActualHeight, ElementName=grdMessageList, Mode=OneWay}">
                    <ScrollViewer.Resources>
                        <sys:Double x:Key="{x:Static SystemParameters.VerticalScrollBarWidthKey}">10</sys:Double>
                    </ScrollViewer.Resources>
                    <Border BorderBrush="#FFFDFDFD"  BorderThickness="2" Width="228">
                        <StackPanel  x:Name="pnlMessages"    MouseRightButtonUp="PnlMessages_MouseLeftButtonUp"  MouseLeftButtonUp="PnlMessages_MouseLeftButtonUp" Margin="-1,0,0,0" />
                    </Border>

                </ScrollViewer>
                <StackPanel Background="#FF917CFF" Panel.ZIndex="1" Margin="0">
                    <TextBlock Text="Message List" HorizontalAlignment="Center" Margin="0,10,0,0" FontSize="14" FontWeight="Bold" Foreground="White" />
                </StackPanel>
                <TextBlock x:Name="txtMessageListState" Text="SHOW" Visibility="Hidden"/>
            </StackPanel>
        </Grid>
    </Grid>
</Window>
