            <TabItem Style="{DynamicResource LeftTabColumnTabItemStyle}"  BorderBrush="{x:Null}">
                <TabItem.Header>
                    <!--<StackPanel Orientation="Horizontal" MouseLeftButtonDown="ShowForbiddenPage" HorizontalAlignment="Left" Height="56">
                        <Label ToolTip="Forbidden" Content="&#xF057;" HorizontalContentAlignment="Center"
                               FontFamily="pack://application:,,,/Resource/#FontAwesome" Foreground="#FF555555"
                               VerticalAlignment="Center" FontSize="30"  Height="38" />
                    </StackPanel>-->
                    <Label ToolTip="Audio" Content="&#xF028;" HorizontalContentAlignment="Center" MouseLeftButtonDown="ShowAudioPage"
                               FontFamily="pack://application:,,,/Resource/#FontAwesome" Foreground="White"
                               VerticalAlignment="Center" FontSize="35"  Margin="-4,0,-3,0" />
                </TabItem.Header>
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>

                    <Grid Grid.Column="0">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*"/>
                        </Grid.RowDefinitions>
                        <Grid Grid.Row="0" Margin="5, 5,5,5" >
                            <Grid.RowDefinitions>
                                <RowDefinition Height="5*"/>
                                <RowDefinition Height="33*"/>
                            </Grid.RowDefinitions>
                            <DataGrid x:Name="AudioInfo" IsReadOnly="True" SelectedItem="Flase" CanUserResizeRows="False" AlternatingRowBackground="#FFe7f4fc" GridLinesVisibility="None" AutoGenerateColumns="False" CanUserResizeColumns="True" CanUserReorderColumns="False" BorderBrush="#FF676868" Margin="-5,-5,-5,-5" FontSize="14" Grid.RowSpan="2">
                                <DataGrid.CellStyle>
                                    <Style TargetType="DataGridCell">
                                        <Setter Property="BorderThickness" Value="1,0,0,1"/>
                                        <Setter Property="BorderBrush" Value="LightBlue"/>
                                    </Style>
                                </DataGrid.CellStyle>

                                <DataGrid.Resources>
                                    <LinearGradientBrush x:Key="HeaderBorderBrush" StartPoint="0.5,0" EndPoint="0.5,1">

                                        <GradientStop Color="#CC4682B4" Offset="0.52" />
                                    </LinearGradientBrush>
                                    <Style BasedOn="{StaticResource {x:Type DataGridColumnHeader}}" TargetType="{x:Type DataGridColumnHeader}">
                                        <Setter Property="Background" Value="{StaticResource HeaderBorderBrush}" />
                                        <Setter Property="Foreground" Value="White"/>
                                        <Setter Property="FontWeight" Value="Bold"/>
                                        <Setter Property="Height" Value="32"/>
                                        <Setter Property="HorizontalContentAlignment" Value="Center" />

                                    </Style>
                                </DataGrid.Resources>
                                <DataGrid.Columns >
                                    <DataGridTextColumn Header="ID" Width="40*" Binding="{Binding ID}">
                                        <DataGridTextColumn.ElementStyle>
                                            <Style TargetType="{x:Type TextBlock}">
                                                <Setter Property="VerticalAlignment" Value="Center" />
                                                <Setter Property="Height" Value="25" />
                                                <Setter Property="HorizontalAlignment" Value="Center" />
                                            </Style>
                                        </DataGridTextColumn.ElementStyle>
                                    </DataGridTextColumn>
                                    <DataGridTextColumn Header="Name" Width="90*" Binding="{Binding UserName}">
                                        <DataGridTextColumn.ElementStyle>
                                            <Style TargetType="{x:Type TextBlock}">
                                                <Setter Property="VerticalAlignment" Value="Center" />
                                                <Setter Property="Height" Value="25" />
                                                <Setter Property="HorizontalAlignment" Value="Center" />
                                                <Setter Property="ToolTip" Value="{Binding UserName}" />
                                            </Style>
                                        </DataGridTextColumn.ElementStyle>
                                    </DataGridTextColumn>
                                    <DataGridTextColumn Header="IP" Width="70*" Binding="{Binding UserIP}">
                                        <DataGridTextColumn.ElementStyle>
                                            <Style TargetType="{x:Type TextBlock}">
                                                <Setter Property="VerticalAlignment" Value="Center" />
                                                <Setter Property="Height" Value="25" />
                                                <Setter Property="HorizontalAlignment" Value="Center" />
                                                <Setter Property="ToolTip" Value="{Binding UserIP}" />
                                            </Style>
                                        </DataGridTextColumn.ElementStyle>
                                    </DataGridTextColumn>
                                    <DataGridTextColumn Header="Start Time" Width="90*" Binding="{Binding From}">
                                        <DataGridTextColumn.ElementStyle>
                                            <Style TargetType="{x:Type TextBlock}">
                                                <Setter Property="VerticalAlignment" Value="Center" />
                                                <Setter Property="Height" Value="25" />
                                                <Setter Property="HorizontalAlignment" Value="Center" />
                                                <Setter Property="ToolTip" Value="{Binding From}" />
                                            </Style>
                                        </DataGridTextColumn.ElementStyle>
                                    </DataGridTextColumn>
                                    <DataGridTextColumn Header="Duration" Width="90*" Binding="{Binding Duration}">
                                        <DataGridTextColumn.ElementStyle>
                                            <Style TargetType="{x:Type TextBlock}">
                                                <Setter Property="VerticalAlignment" Value="Center" />
                                                <Setter Property="Height" Value="25" />
                                                <Setter Property="HorizontalAlignment" Value="Center" />
                                                <Setter Property="ToolTip" Value="{Binding Duration}" />
                                            </Style>
                                        </DataGridTextColumn.ElementStyle>
                                    </DataGridTextColumn>
                                    <DataGridTextColumn Header="Player Title" Width="130*" Binding="{Binding ProcessWindow}">
                                        <DataGridTextColumn.ElementStyle>
                                            <Style TargetType="{x:Type TextBlock}">
                                                <Setter Property="VerticalAlignment" Value="Center" />
                                                <Setter Property="Height" Value="25" />
                                                <Setter Property="HorizontalAlignment" Value="Center" />
                                                <Setter Property="ToolTip" Value="{Binding ProcessWindow}" />
                                            </Style>
                                        </DataGridTextColumn.ElementStyle>
                                    </DataGridTextColumn>
                                    <DataGridTextColumn Header="Audio Name" Width="90*" Binding="{Binding FileName}">
                                        <DataGridTextColumn.ElementStyle>
                                            <Style TargetType="{x:Type TextBlock}">
                                                <Setter Property="VerticalAlignment" Value="Center" />
                                                <Setter Property="Height" Value="25" />
                                                <Setter Property="HorizontalAlignment" Value="Center" />
                                                <Setter Property="ToolTip" Value="{Binding FileName}" />
                                            </Style>
                                        </DataGridTextColumn.ElementStyle>
                                    </DataGridTextColumn>
                                    <DataGridTextColumn Header="Size" Width="90*" Binding="{Binding Size}">
                                        <DataGridTextColumn.ElementStyle>
                                            <Style TargetType="{x:Type TextBlock}">
                                                <Setter Property="VerticalAlignment" Value="Center" />
                                                <Setter Property="Height" Value="25" />
                                                <Setter Property="HorizontalAlignment" Value="Center" />
                                                <Setter Property="ToolTip" Value="{Binding Size}" />
                                            </Style>
                                        </DataGridTextColumn.ElementStyle>
                                    </DataGridTextColumn>
                                    <DataGridTemplateColumn  Header="" Width="33" >
                                        <DataGridTemplateColumn.CellTemplate>
                                            <DataTemplate>
                                                <!--<Button Name="Play" Content="View" Click="btnPlay_Click" />-->

                                                <Button Name="Play" ToolTip="Play" Content="&#xF04b;" Style="{DynamicResource RoundedSettingsButtonStyle1}"
                        BorderBrush="Transparent" Background="#FFE8850E" Foreground="White"
                        FontFamily="pack://application:,,,/Resource/#FontAwesome" FontSize="15"
                        VerticalContentAlignment="Center" Click="btnPlay_Click" Focusable="False" />
                                            </DataTemplate>
                                        </DataGridTemplateColumn.CellTemplate>
                                    </DataGridTemplateColumn>

                                </DataGrid.Columns>
                            </DataGrid>
                        </Grid>
                    </Grid>
                </Grid>
            </TabItem>
            <TabItem Style="{DynamicResource LeftTabColumnTabItemStyle}" BorderBrush="{x:Null}">
                <TabItem.Header>
                    <!--<StackPanel Orientation="Horizontal" MouseLeftButtonDown="ShowURLPage" HorizontalAlignment="Left" Margin="0,0,0,0" Height="58">
                        <Label ToolTip="URL" Content="&#xf09e;" HorizontalContentAlignment="Center"
                               FontFamily="pack://application:,,,/Resource/#FontAwesome" Foreground="#FF555555"
                               VerticalAlignment="Center" FontSize="30"  Height="38"/>
                    </StackPanel>-->
                    <Label ToolTip="URL" Content="&#xf09e;" HorizontalContentAlignment="Center" MouseLeftButtonDown="ShowURLPage"
                               FontFamily="pack://application:,,,/Resource/#FontAwesome" Foreground="White"
                               VerticalAlignment="Center" FontSize="35"  Margin="-5,0" />
                </TabItem.Header>
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="230" />
                        <ColumnDefinition Width="*" />
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>
                    <Border Grid.Column="0" BorderThickness="1" Background="White" >
                        <StackPanel Height="35" VerticalAlignment="Top" Background="#EE383838">
                            <Label FontSize="18" HorizontalAlignment="Center" Foreground="#FFFFFF" FontWeight="SemiBold">Members List</Label>
                        </StackPanel>
                    </Border>
                    <ScrollViewer Grid.Column="0" BorderThickness="5" VerticalScrollBarVisibility="Hidden" Background="White" Margin="0,35,0,0" >
                        <StackPanel x:Name="pnlURL_Client_List" >
                        </StackPanel>
                    </ScrollViewer>
                    <Grid Grid.Column="1">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="10*"/>
                            <RowDefinition Height="170*"/>
                        </Grid.RowDefinitions>
                        <Grid Grid.Row="0">
                            <TextBox x:Name="urlFilter" Height="23" Margin="7,1,450,1" TextWrapping="Wrap" Text="Filter by url name." VerticalAlignment="Center" VerticalContentAlignment ="Center"
                                Style="{DynamicResource SearchTextBoxStyle}"
                                ToolTip="Filter" TabIndex="1" FontSize="14" IsEnabled="{Binding IsAvailable}"
                                 GotFocus="URLFilter_GotFocus" LostFocus="URLFilter_LostFocus" KeyDown="OnKeyURLHandler"/>

                            <CheckBox Name="chURLFilter" Margin="570,5,1,0" HorizontalAlignment="Right" Click="URLCheckBox_Click">
                                <CheckBox.LayoutTransform>
                                    <ScaleTransform ScaleX="1.5" ScaleY="1.5" />
                                </CheckBox.LayoutTransform>

                                <CheckBox.Content>
                                    <Grid>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="20*"/>
                                            <ColumnDefinition Width="30*"/>
                                        </Grid.ColumnDefinitions>
                                        <Grid Grid.Column="0">
                                            <Label DockPanel.Dock="Left" Margin="-3, -3, 0, 0" Content="&#xf09e;" HorizontalContentAlignment="Center"
                                                FontFamily="pack://application:,,,/Resource/#FontAwesome" Foreground="Black"
                                                VerticalAlignment="Center" FontSize="15" />
                                        </Grid>
                                        <Grid Grid.Column="1">
                                            <Label Content="Filter by danger url " Margin="-3, -5, 0, -2" FontSize="10px" HorizontalAlignment="Left" VerticalContentAlignment="Center"/>
                                        </Grid>
                                    </Grid>
                                </CheckBox.Content>
                            </CheckBox>

                        </Grid>
                        <Grid Grid.Row="1" Margin="5, 2,5,5">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="67*"/>
                                <ColumnDefinition Width="16*"/>
                            </Grid.ColumnDefinitions>
                            <DataGrid x:Name="MemeberURL" GridLinesVisibility="None" CanUserResizeRows="False" IsReadOnly="True" SelectedItem="{Binding SelectedRow, Mode=TwoWay}"  AutoGenerateColumns="True" CanUserResizeColumns="True" CanUserReorderColumns="True" BorderBrush="#FF676868" Margin="0,0,0,0" FontSize="14" ContextMenuOpening="MemeberURL_ContextMenuOpening" Grid.ColumnSpan="2">
                                <DataGrid.ContextMenu>
                                    <ContextMenu x:Name="menu" BorderBrush="Black" BorderThickness="1">
                                        <MenuItem Header="Copy URL" Click="CopyURL_Click" />
                                        <MenuItem Header="Go to URL" Click="GoToURL_Click" />
                                    </ContextMenu>
                                </DataGrid.ContextMenu>

                                <DataGrid.Resources>
                                    <Style TargetType="{x:Type DataGridCell}">
                                        <Style.Triggers>
                                            <Trigger Property="IsSelected" Value="True">
                                                <Setter Property="Background" Value="{DynamicResource {x:Static SystemColors.HighlightBrushKey}}"/>
                                                <Setter Property="Foreground" Value="{DynamicResource {x:Static SystemColors.HighlightTextBrushKey}}"/>
                                                <Setter Property="BorderBrush" Value="{DynamicResource {x:Static SystemColors.HighlightBrushKey}}"/>
                                            </Trigger>
                                            <MultiDataTrigger>
                                                <MultiDataTrigger.Conditions>
