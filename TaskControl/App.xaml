<Application x:Class="Monitor.TaskControl.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Monitor.TaskControl">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="UserControl/CommonDictionary.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <FontFamily x:Key="FontAwesome">/Resource/fa-solid-900.ttf#Font Awesome 5 Free Solid</FontFamily>
        </ResourceDictionary>
    </Application.Resources>
</Application>
